{
  "name": "@sgtpooki/streams-interop",
  "version": "1.3.0",
  "description": "Interop tests for @sgtpooki/streams-*",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/sgtpooki/streams/tree/main/packages/interop#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgtpooki/streams.git"
  },
  "bugs": {
    "url": "https://github.com/sgtpooki/streams/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "sgtpooki",
    "streams",
    "interop"
  ],
  "bin": {
    "sgtpooki-streams-interop": "./dist/src/bin.js"
  },
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main"
  },
  "dependencies": {
    "@sgtpooki/streams-transform-slice": "0.0.0",
    "aegir": "^44.0.0"
  },
  "browser": {
  }
}
