{
  "name": "@sgtpooki/streams-transform-slice",
  "version": "0.0.0",
  "description": "A transform stream that accepts a ReadableStream, offsets and length and returns a ReadableStream containing only the specified slice of the input stream.",
  "license": "Apache-2.0 OR MIT",
  "homepage": "https://github.com/sgtpooki/streams/tree/main/packages/transform-slice#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sgtpooki/streams.git"
  },
  "bugs": {
    "url": "https://github.com/sgtpooki/streams/issues"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "keywords": [
    "sgtpooki",
    "streams",
    "slice",
    "splice",
    "copyWithin",
    "transform"
  ],
  "type": "module",
  "types": "./dist/src/index.d.ts",
  "files": [
    "src",
    "dist",
    "!dist/test",
    "!**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "eslintConfig": {
    "extends": "ipfs",
    "parserOptions": {
      "project": true,
      "sourceType": "module"
    }
  },
  "scripts": {
    "clean": "aegir clean",
    "lint": "aegir lint",
    "dep-check": "aegir dep-check",
    "doc-check": "aegir doc-check",
    "build": "aegir build",
    "test": "aegir test",
    "test:chrome": "aegir test -t browser --cov",
    "test:chrome-webworker": "aegir test -t webworker",
    "test:firefox": "aegir test -t browser -- --browser firefox",
    "test:firefox-webworker": "aegir test -t webworker -- --browser firefox",
    "test:node": "aegir test -t node --cov",
    "test:electron-main": "aegir test -t electron-main",
    "release": "aegir release"
  },
  "dependencies": {
    "@libp2p/logger": "^4.0.7"
  },
  "devDependencies": {
    "aegir": "^42.2.5",
    "it-to-browser-readablestream": "^2.0.6",
    "sinon": "^17.0.1",
    "sinon-ts": "^2.0.0"
  },
  "browser": {
    "./dist/src/node-transform-stream.js": false,
    "fs": false,
    "path": false,
    "url": false,
    "stream": false
  },
  "sideEffects": false
}
